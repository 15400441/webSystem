<div class="container-fluid">
	<div class="col-md-12">
		<div class="row">
			<div class="col-md-12">
				<% activities.forEach( function(model) { %>
				<div class="col-md-6">
					<div class="panel panel-default" style="height:200px;">
						<div class="panel-heading">
							<a href="/activity/detail?id=<%=model.id%>"><h3 class="panel-title"><%=model.name%></h3>
							</a>
						</div>
						<div class="panel-body">
							<div class="col-md-4">
								<img alt="Bootstrap Image Preview" src=<%=model.image%> style="height:135px;object-fit:cover;" />
							</div>
							<div class="col-md-8" >
								<p style="min-height:140px;max-height:140px;overflow:hidden">
									<%=model.fullDes%>
								</p>
							</div>
						</div>
					</div>
				</div>
				<% }); %>
			</div>
		</div>
	</div>
</div>