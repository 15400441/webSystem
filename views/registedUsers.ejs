<div class="container-fluid">
	<div class="col-md-12">
		<div class="row">
			<div class="col-md-12">
				<table class="table">
					<thead>Registers of <%= activity.name%></thead>
					<tbody>
						<% activity.belongsTo.forEach( function(model) { %>
						<tr>
							<td><%=model.username%><td>
							<td><td>
							<td><td>
							<td><td>
							<td><a href="/activity/deleteRegister?uid=<%=model.id%>">delete</a></td>
							
						</tr>
						<% }); %>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>